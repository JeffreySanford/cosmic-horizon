{
  "name": "@vlass-portal/vlass-api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "vlass-api-e2e",
    "implicitDependencies": [
      "vlass-api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/vlass-api-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "vlass-api:build",
          "vlass-api:serve-e2e"
        ]
      }
    }
  }
}
